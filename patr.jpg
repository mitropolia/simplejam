<script type="text/javascript">
var casenum = '2';
var sw_mode = '1';
var wlc_psta = '0';
var wlc_express = '0';
if( ((sw_mode == 2 || sw_mode == 3) && wlc_psta == 1) || (sw_mode == 3 && wlc_psta == 3))
sw_mode = 4;
if(sw_mode == 3 && wlc_psta == 2)
sw_mode = 2;
var wanstate = -1;
var wansbstate = -1;
var wanauxstate = -1;
wanstate = 4;
wansbstate = 0;
wanauxstate = 0;

var isSupport = function(_ptn){return ('mssid 2.4G 5G update usbX2 switchctrl manual_stb pwrctrl WIFI_LOGO nandflash meoVoda movistarTriple app reboot_schedule ipv6 ipv6pt PARENTAL2 dualwan pptpd openvpnd utf8_ssid printer modem webdav rrsut cloudsync media appbase timemachine hdspindown diskutility frs_feedback dblog email bwdpi wrs_wbl ookla HTTPS letsencrypt ssh vpnc repeater psta wl6 optimize_xbox wifi_tog_btn user_low_rssi bcmfa tcode usericon stainfo cloudcheck realip cfg_sync fupgrade amas bcmwifi conndiag eula proxysta account_binding gameMode'.search(_ptn) == -1) ? false : true;}
var tmo_support = isSupport("tmo");
var dsl_support = isSupport("dsl");
var gobi_support = isSupport("gobi");
var wans_dualwan_orig = 'wan none';
var wans_flag = (wans_dualwan_orig.search("none") == -1) ? 1 : 0;
if(wans_dualwan_orig.search(" ") == -1)
wans_flag = 0;
var new_lan_ip = [{next_lanip: '192.168.1.1'}][0].next_lanip;
var header_info = [{ "host": "router.asus.com", "current_page": "error_page.htm", "protocol": "http", "port": 80 }];
var location_org = header_info[0].protocol + "://" + header_info[0].host + ":" + header_info[0].port;
var odm_support = ('mssid 2.4G 5G update usbX2 switchctrl manual_stb pwrctrl WIFI_LOGO nandflash meoVoda movistarTriple app reboot_schedule ipv6 ipv6pt PARENTAL2 dualwan pptpd openvpnd utf8_ssid printer modem webdav rrsut cloudsync media appbase timemachine hdspindown diskutility frs_feedback dblog email bwdpi wrs_wbl ookla HTTPS letsencrypt ssh vpnc repeater psta wl6 optimize_xbox wifi_tog_btn user_low_rssi bcmfa tcode usericon stainfo cloudcheck realip cfg_sync fupgrade amas bcmwifi conndiag eula proxysta account_binding gameMode'.indexOf('odm') != -1) ? true : false;
function initial(){
var is_logined = ["1"][0];
var html_code = '';
var url_flag = "";
var html_code_notlogin = "<ul><li>Пожалуйста, обратитесь к администратору сети.</li></ul>";
var showtext = function(obj, str){if(obj)obj.innerHTML=str;}
if(odm_support){
$('.model_img').attr('class', 'mode_img_COD');
}
if(casenum == 1){
$("#page_title").html("Сетевой кабель не подключен");
html_code += "<ul>";
html_code += "<li>Убедитесь, что кабель соединяет WAN порт вашего RT-AC68U и модем.</li>";
html_code += "<li>Проверьте, нужно ли изменить <a style='color:#FFF' href='/Advanced_OperationMode_Content.asp'>Режим работы</a>.</li>";
html_code += "</ul>";
$("#wanLink").click(function(){
detect_general();
});
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggestion").hide();
}
else if(casenum == 2){
html_code += "<ul>";
html_code += "<li>";
if(wansbstate == 2 || wanauxstate == 3){ //Authentication Failed
$("#page_title").html("Ошибка аутентификации PPPoE");
html_code += "<div>Неверная учетная запись PPPoE или пароль. <a style='color:#FFF' href='/Advanced_WAN_Content.asp'>Проверить</a>.</div>";
$("#wanLink").click(function(){
});
}
else if(wansbstate == 1){ //No Response
}
else if(wansbstate == 10){ //Terminal due to lack of activity
}
else{ //Connecting
$("#animation_field").show();
detect_ppp_connect();
$("#page_title").html("Повторное подключение к сети Интернет.");
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggest_title").hide();
$("#suggestion").hide();
$("#case_content").hide();
$("#back_btn").show();
$("#wanLink").click(function(){
history.go(-1);
});
$("#wanLink").html("Предыдущая страница");
}
html_code += "</li>";
html_code += "</ul>";
$("#wanLink").hide();
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggestion").hide();
}
else if(casenum == 3){
$("#page_title").html("Невозможно подключиться к Интернету");
html_code += "<ul>";
html_code += "<li>Убедитесь, что <a style='color:#FFF' href='/Advanced_WAN_Content.asp'>Настройки подключения</a> указаны правильно.</li>";
html_code += "<li>Свяжитесь со своим поставщиком услуг Интернет, чтобы узнать основную причину сбоя DHCP.</li>";
html_code += "</ul>";
$("#wanLink").click(function(){
detect_general();
});
$("#wanLink").html("Изменить тип WAN");
$("#wanLink").hide();
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggestion").hide();
}
else if(casenum == 4){
showtext($("#failReason1")[0], "Ошибка подключения к сети Интернет. IP-адрес совпадает с IP-адресом шлюза");
html_code += "<ul>";
html_code += "<li>Пожалуйста, сконфигурируйте IP-адрес и убедитесь, что он отличается от IP-адреса шлюза</li>";
html_code += "</ul>";
$("#wanLink").click(function(){
detect_general();
});
}
else if(casenum == 5){
if(wanstate == 5){
$("#page_title").html("WAN-подключение было отключено.");
}
else{
$("#page_title").html("USB-модем не подключен.");
}
html_code += '<ul>';
if(wanstate == 5){
html_code += "<li>Включите <a style='color:#FFF' href='/'>WAN-подключение</a></li>";
}
else{
html_code += "<li>Убедитесь, что USB-модем включен.</li>";
html_code += "<li>Измените <a style='color:#FFF' href='/Advanced_Modem_Content.asp'>Настройки USB-модема</a>.</li>";
}
html_code += '</ul>';
if(wanstate == 5){
$("#wanLink").click(function(){
manually_start_wan_Link();
});
}else{
$("#wanLink").click(function(){
detect_general();
});
}
$("#wanLink").hide();
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggestion").hide();
}
else if(casenum.search("6") !== -1){
$("#page_title").html("Обнаружен конфликт IP-адресов.");
html_code += "<ul>";
html_code += "<li>Нажмите [Далее] для получения нового IP адреса. Новый IP-адрес:";
html_code += "<span style='font-weight:bolder'>"+ new_lan_ip +"</span></li>";
html_code += "<li>Убедитесь, что все соответствующие службы используют новый IP-адрес (например, переадресация портов, DMZ и т. д.)</li>";
html_code += "</ul>";
$("#wanLink").html("Далее");
$("#wanLink").click(function(){
change_lan_subnet();
});
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggestion").hide();
}
else if(casenum == 8){
$("#page_title").html("Невозможно подключиться к родительской точке доступа.");
if(!is_logined){
html_code = html_code_notlogin;
$("#wanLink").hide();
}
else{
html_code += "<ul>";
html_code += "<li>Убедитесь, что родительская точка доступа может подключиться к сети Интернет.</li>";
html_code += "<li><a style='color:#FFF' href='/QIS_wizard.htm?flag=manual'>Изменить настройки подключения</a>.</li>";
html_code += "</ul>";
if(sw_mode == "2"){
if(wlc_express == '1')
url_flag += "sitesurvey_exp2";
else if(wlc_express == '2')
url_flag += "sitesurvey_exp5";
else
url_flag += "sitesurvey_rep";
}
else
url_flag += "sitesurvey_mb";
$("#wanLink").click(function(){
top.location.href = "/QIS_wizard.htm?flag=" + url_flag;
});
}
$("#wanLink").hide();
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggestion").hide();
}
else if(casenum == 9) {
$("#page_title").html("Невозможно подключиться к родительской точке доступа.");
html_code += "<ul>";
html_code += "<li>Убедитесь, что роутер AiMesh включен.</li>";
html_code += "<li>Перезагрузите роутер AiMesh и повторите попытку.</li>";
html_code += "</ul>";
$("#wanLink").hide();
$("#reason_title").hide();
$("#reason_field").hide();
$("#suggestion").hide();
}
else{
parent.location = '/';
}
$("#case_content")[0].innerHTML = html_code;
if(casenum == "6_1"){
change_lan_subnet();
}
var windowHeight = (function(){
if(window.innerHeight)
return window.innerHeight;
else if(document.body && document.body.clientHeight)
return document.body.clientHeight;
else if(document.documentElement && document.documentElement.clientHeight)
return document.documentElement.clientHeight;
else
return 800;
})();
document.getElementById("loginTable").style.display = "";
}
function pppoeLink(){
top.location.href = "/QIS_wizard.htm?flag=pppoe";
}
function detectLink(){
top.location.href = "/QIS_wizard.htm?flag=detect";
}
function manually_start_wan_Link(){
top.location.href = '/index.asp'+"?flag=Internet";
}
function change_lan_subnet(){
top.location.href = "/updateSubnet.htm";
}
function detect_router(){
$.ajax({
url: location_org.replace(header_info[0].host, new_lan_ip) + "/repage.json",
dataType: "script",
timeout: 5000,
success: function(){
top.location.href = location_org.replace(header_info[0].host, new_lan_ip)
+ "/QIS_wizard.htm?flag="
+ (function(){return (('1' === '0')?"welcome":"detect")})();
},
error: function(){
setTimeout(detect_router, 1000);
setTimeout(function(){
$("#case_content ul").html("<li>IP адрес был обновлен. Если процесс более двух минут, пожалуйста выполните следующие инструкции для обновления IP вручную: (1) отключите сетевой кабель; (2) подождите десять секунд; (3) подключите его обратно для полученияя нового IP адреса вручную.</li>");
}, 3000);
}
});
}
function detect_general(){
if(wans_flag){ //Dual WAN
if(wans_dualwan_orig.split(" ")[0].toUpperCase() == "USB"){
if(gobi_support)
top.location.href = '/Advanced_MobileBroadband_Content.asp';
else
top.location.href = '/Advanced_Modem_Content.asp';
}else if(dsl_support){
if(wans_dualwan_orig.split(" ")[0].toUpperCase() == "WAN" || wans_dualwan_orig.split(" ")[0].toUpperCase() == "LAN")
top.location.href = '/Advanced_WAN_Content.asp';
else
top.location.href = '/Advanced_DSL_Content.asp';
}
else
top.location.href = '/Advanced_WAN_Content.asp';
}
else{ //Single WAN
if(dsl_support)
top.location.href = '/Advanced_DSL_Content.asp';
else if(wans_dualwan_orig.split(" ")[0].toUpperCase() == "USB")
top.location.href = '/Advanced_MobileBroadband_Content.asp';
else if(sw_mode == 3)
top.location.href = '/';
else
top.location.href = '/Advanced_WAN_Content.asp';
}
}
function detect_ppp_connect(){
$.ajax({
url: '/WAN_info.asp',
dataType: 'script',
error: function(xhr){
setTimeout(detect_ppp_connect, 1000);
},
success: function(response){
if( wanstate == "2" && wansbstate == "0" && wanauxstate == "0"){
top.location.href = '/';
}
else{
setTimeout(detect_ppp_connect, 1000);
}
}
});
}
</script>
